{
  "name": "adaptive-material-ui",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "sideEffects": false,
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "module": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./adaptiveMode": {
      "module": "./dist/adaptiveMode/index.js",
      "types": "./dist/adaptiveMode/index.d.ts"
    },
    "./*": {
      "module": "./dist/*.js",
      "types": "./dist/*.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rslib build",
    "format": "prettier --write .",
    "lint": "eslint .",
    "start": "rslib build --watch",
    "test": "vitest",
    "test-ct": "playwright test -c playwright-ct.config.ts",
    "test-ct-debug": "playwright test --ui -c playwright-ct.config.ts"
  },
  "devDependencies": {
    "@emotion/react": "11.14.0",
    "@emotion/styled": "11.14.1",
    "@eslint/js": "9.31.0",
    "@playwright/experimental-ct-react": "1.54.1",
    "@rsbuild/plugin-react": "1.3.4",
    "@rslib/core": "0.11.0",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "16.3.0",
    "@trivago/prettier-plugin-sort-imports": "5.2.2",
    "@types/node": "24.1.0",
    "@types/react": "19.1.8",
    "@types/react-dom": "19.1.6",
    "eslint": "9.31.0",
    "eslint-import-resolver-typescript": "4.4.4",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "5.2.0",
    "jsdom": "26.1.0",
    "prettier": "3.6.2",
    "react": "19.1.0",
    "typescript": "5.8.3",
    "typescript-eslint": "8.38.0",
    "vitest": "3.2.4"
  },
  "peerDependencies": {
    "@mui/material": "^7.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "dependencies": {
    "@mui/utils": "7.2.0",
    "clsx": "2.1.1"
  }
}
