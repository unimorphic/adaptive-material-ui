{
  "name": "adaptive-material-ui",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "sideEffects": false,
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "module": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./adaptiveMode": {
      "module": "./dist/adaptiveMode/index.js",
      "types": "./dist/adaptiveMode/index.d.ts"
    },
    "./*": {
      "module": "./dist/*.js",
      "types": "./dist/*.d.ts"
    }
  },
  "scripts": {
    "build": "rslib build",
    "format": "prettier --write .",
    "lint": "eslint .",
    "publish": "npm run build && node scripts/cleanAndCopyPackageJson.cjs && cd dist && npm pack",
    "start": "rslib build --watch",
    "test": "vitest",
    "test-ct": "playwright test -c playwright-ct.config.ts",
    "test-ct-debug": "playwright test --ui -c playwright-ct.config.ts"
  },
  "devDependencies": {
    "@eslint/js": "9.38.0",
    "@playwright/experimental-ct-react": "1.56.1",
    "@rsbuild/plugin-react": "1.4.1",
    "@rslib/core": "0.16.1",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/react": "16.3.0",
    "@trivago/prettier-plugin-sort-imports": "5.2.2",
    "@types/node": "24.9.1",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2",
    "eslint": "9.38.0",
    "eslint-import-resolver-typescript": "4.4.4",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "7.0.0",
    "jsdom": "27.0.1",
    "prettier": "3.6.2",
    "typescript": "5.9.3",
    "typescript-eslint": "8.46.2",
    "vitest": "4.0.2"
  },
  "peerDependencies": {
    "@emotion/react": "^11.5.0",
    "@emotion/styled": "^11.3.0",
    "@mui/material": "^7.3.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "dependencies": {
    "@mui/system": "7.3.3",
    "@mui/utils": "7.3.3",
    "clsx": "2.1.1"
  },
  "peerDependenciesMeta": {
    "@emotion/react": {
      "optional": true
    },
    "@emotion/styled": {
      "optional": true
    }
  }
}
